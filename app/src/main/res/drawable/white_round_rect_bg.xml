<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <solid android:color="@color/translucent_main2"/>
            <corners android:radius="15dp"/>
        </shape>
    </item>
    <item android:top="0.1mm"
        android:bottom="0.1mm"
        android:left="0.1mm"
        android:right="0.1mm">
        <shape>
            <solid android:color="@color/white"/>
            <corners android:radius="15dp"/>
            <stroke android:color="#096072ff"
                android:width="1dp"/>
            <padding android:bottom="3dp"
                android:top="3dp"
                android:left="2dp"
                android:right="2dp"/>
        </shape>
    </item>

</layer-list>